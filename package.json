{
  "name": "hardware-monitor",
  "version": "0.1.0",
  "author": "Jens Streck",
  "description": "Hardware monitor written in Node JS",
  "main": "electron/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.0.0-alpha2",
    "chart.js": "^2.9.3",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.2.0",
    "history": "^5.0.0",
    "immutability-helper": "^3.1.1",
    "moment": "^2.29.1",
    "node-sass": "^7.0.0",
    "popper.js": "^1.16.1",
    "react": "^17.0.0-rc.2",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "^17.0.0-rc.2",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "^3.4.3",
    "react-vis": "^1.11.7",
    "socket.io-client": "^2.3.1"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --env development --hot",
    "start:old": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "build": "npm run react-build && npm run electron-build",
    "electron-build": "electron-builder"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "wait-on": "^5.2.0",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^6.0.2",
    "copy-webpack-plugin": "^6.2.0",
    "css-loader": "^4.3.0",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.11.3",
    "postcss-loader": "^4.0.3",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.1.4",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "build": {
    "appId": "Hardware Monitor",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "icon": "build/icon.png",
      "target": "portable"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
